{
  "name": "CyberDefense",
  "namespace": "cyberdefense",
  "version": "0.0.1",
  "main": "server.js",
  "description": "UWF Codefest 2016 CyberDefense app",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "build": "NODE_ENV=production gulp build",
    "build:dev": "NODE_ENV=development gulp build",
    "dev": "NODE_ENV=development gulp watch",
    "prod": "NODE_ENV=production gulp watch",
    "dev:dbg": "NODE_ENV=development DEBUG=app:* gulp watch",
    "deploy": "NODE_ENV=production gulp build && gulp deploy",
    "test": "NODE_ENV=testing gulp test",
    "test:dbg": "NODE_ENV=testing DEBUG=app:*,test:* gulp test"
  },
  "dependencies": {
    "async": "^0.9.2",
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.14.1",
    "chalk": "^0.5.1",
    "classnames": "^2.2.0",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "glob": "^4.5.3",
    "lodash": "^3.10.1",
    "mandrill-api": "^1.0.45",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "node-uuid": "^1.4.7",
    "q": "^1.4.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "request": "^2.69.0",
    "validate.js": "^0.9.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.8",
    "babelify": "^7.2.0",
    "bower-resolve": "^2.2.1",
    "browserify": "^12.0.1",
    "browserify-shim": "^3.8.11",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-lodash": "^0.1.3",
    "eslint-plugin-react": "^3.3.1",
    "event-stream": "^3.3.1",
    "gulp": "^3.9.0",
    "gulp-bytediff": "^0.2.1",
    "gulp-concat": "^2.5.0",
    "gulp-filesize": "^0.0.6",
    "gulp-if": "^1.2.5",
    "gulp-inject": "^1.1.1",
    "gulp-load-plugins": "^0.9.0",
    "gulp-minify-css": "^0.4.5",
    "gulp-mocha": "^2.0.1",
    "gulp-nodemon": "^2.0.2",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.0.4",
    "gulp-tap": "^0.1.3",
    "gulp-uglify": "^1.1.0",
    "mocha-unfunk-reporter": "^0.4.0",
    "ncp": "^2.0.0",
    "node-notifier": "^4.5.0",
    "rimraf": "^2.4.4",
    "run-sequence": "^1.1.4",
    "semver": "^4.3.6",
    "supertest": "1.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0",
    "yargs": "^3.26.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "semantic": "./bower_components/semantic/dist/semantic.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "semantic": {
      "exports": "semantic",
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "paths": {
    "src": {
      "app": {
        "js": [
          "public/app/**/*.js",
          "public/app/**/*.jsx"
        ],
        "scss": [
          "public/app/**/*.scss"
        ],
        "test": [
          "test/globals.js",
          "test/utils.js",
          "server/test/**/*.js",
          "server/**/test.js"
        ]
      },
      "vendor": {
        "deps": [
          "classnames",
          "react",
          "react-dom",
          "react-router",
          "lodash",
          "q",
          "validate.js",
          "moment"
        ],
        "libs": [
          "jquery",
          "semantic"
        ],
        "css": [
          "bower_components/semantic/dist/semantic.css"
        ],
        "fonts": "bower_components/semantic/dist/themes/default/assets/fonts"
      }
    },
    "dist": {
      "app": {
        "js": {
          "file": "app.bundle.js",
          "dir": "assets/js",
          "entryPoint": "public/app/index.js"
        },
        "css": {
          "file": "app.bundle.css",
          "dir": "assets/css"
        }
      },
      "vendor": {
        "js": {
          "file": "vendor.bundle.js",
          "dir": "assets/js"
        },
        "css": {
          "file": "vendor.bundle.css",
          "dir": "assets/css"
        },
        "fonts": "assets/fonts"
      }
    }
  }
}
